					Av. Greenville 987,
					%br/
					New York,
					%br/
					90873
					%br/
					United States
			.col-lg-7
				%h3 Drop Us A Line
				%br/
				%form{action: "#", enctype: "plain", method: "post", role: "form"}
					.form-group
						%label{for: "name1"} Your Name
						%input#name1.form-control{name: "Name", placeholder: "Your Name", type: "name"}/
					.form-group
						%label{for: "email1"} Email address
						%input#email1.form-control{name: "Mail", placeholder: "Enter email", type: "email"}/
					.form-group
						%label Your Text
						%textarea.form-control{name: "Message", rows: "3"}
					%br/
					%button.btn.btn-large.btn-success{type: "submit"} SUBMIT
