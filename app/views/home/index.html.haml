	%section#showcase{name: "showcase"}
	#showcase
		.container
			.row
				%h1.centered Some Screenshots
				%br/
				.col-lg-8.col-lg-offset-2
					#carousel-example-generic.carousel.slide
						/ Indicators
						%ol.carousel-indicators
							%li.active{"data-slide-to" => "0", "data-target" => "#carousel-example-generic"}
							%li{"data-slide-to" => "1", "data-target" => "#carousel-example-generic"}
						/ Wrapper for slides
						.carousel-inner
							.item.active
								%img{alt: "", src: "assets/img/item-01.png"}/
							.item
								%img{alt: "", src: "assets/img/item-02.png"}/
			%br/
